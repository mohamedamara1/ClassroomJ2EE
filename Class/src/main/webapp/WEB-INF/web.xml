  </servlet>
  <servlet>
  	<servlet-name>AuthorizationServlet</servlet-name>
  	<display-name>AuthorizationServlet</display-name>
  	<description></description>
  	<servlet-class>test.AuthorizationServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AuthorizationCallbackServlet</servlet-name>
  	<display-name>AuthorizationCallbackServlet</display-name>
