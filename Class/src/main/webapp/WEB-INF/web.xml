  	<display-name>DownloadFileServlet</display-name>
  	<description></description>
  	<servlet-class>test.DownloadFileServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Welcome</servlet-name>
  	<url-pattern>/Welcome</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Oauth2callback</servlet-name>
  	<url-pattern>/Oauth2callback</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AuthorizationServlet</servlet-name>
  	<url-pattern>/AuthorizationServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AuthorizationCallbackServlet</servlet-name>
  	<url-pattern>/AuthorizationCallbackServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RequestCredentials</servlet-name>
  	<url-pattern>/RequestCredentials</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DownloadFileServlet</servlet-name>
  	<url-pattern>/DownloadFileServlet</url-pattern>
  </servlet-mapping>
